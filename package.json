    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.11",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
